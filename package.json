{
  "name": "quick-parcel-project",
  "version": "2.3.0",
  "description": "Quick parcel project package",
  "default": "index.html",
  "repository": "https://github.com/pantaflex44/QuickParcelProject",
  "author": "Christophe LEMOINE",
  "license": "MIT",
  "dependencies": {
    "@mantine/core": "^3.6.5",
    "@mantine/dates": "^3.6.5",
    "@mantine/dropzone": "^3.6.5",
    "@mantine/hooks": "^3.6.5",
    "@mantine/modals": "^3.6.5",
    "@mantine/notifications": "^3.6.5",
    "@mantine/prism": "^3.6.5",
    "@mantine/rte": "^3.6.5",
    "connect": "^3.7.0",
    "dayjs": "^1.10.7",
    "parcel": "^2.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "redux": "^4.1.2",
    "sass": "^1.48.0",
    "serve-static": "^1.14.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@emailjs/browser": "^3.4.0",
    "@parcel/babel-preset-env": "^2.2.1",
    "@parcel/transformer-sass": "^2.2.0",
    "@prismicio/client": "^6.1.1",
    "@prismicio/react": "^2.0.5",
    "@testing-library/dom": "^8.11.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.4.0",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.4.6",
    "eslint-plugin-react-hooks": "^4.3.0",
    "helmet": "^5.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "jest-transform-stub": "^2.0.0",
    "lodash.escaperegexp": "^4.1.2",
    "prismic": "^0.0.1",
    "react-helmet-async": "^1.2.2",
    "react-icons": "^4.3.1",
    "react-router-transition": "^2.1.0",
    "react-scripts": "^5.0.0",
    "react-test-renderer": "^17.0.2",
    "react-transition-group": "^4.4.2",
    "replace-in-file": "^6.3.2",
    "spinners-react": "^1.0.6"
  },
  "scripts": {
    "dev:clean": "rm -rf pre-dist/*",
    "dev:parcel": "NODE_ENV=development parcel ./src/index.html --open --dist-dir ./pre-dist",
    "build:clean": "rm -rf dist/*",
    "build:parcel": "NODE_ENV=production parcel build ./src/index.html --public-url ./ --dist-dir ./dist --no-source-maps && node ./postbuild.js",
    "clean": "npm run dev:clean && npm run build:clean",
    "dev": "npm run dev:clean && npm run dev:parcel",
    "build": "npm run build:clean && npm run build:parcel",
    "test": "jest",
    "test-watch": "jest --watchAll",
    "serve": "node serve.js"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "flexbox": "no-2009"
      }
    }
  },
  "babel": {
    "presets": [
      "@parcel/babel-preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "verbose": true,
    "clearMocks": true,
    "coverageDirectory": "coverage",
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "./tests.config.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "jest-transform-stub",
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "jest-transform-stub"
    }
  }
}
